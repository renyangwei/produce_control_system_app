<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.papermanagement">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/logo"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".activity.LoginActivity"
            android:screenOrientation="landscape" />
        <activity
            android:name=".activity.RealTimeDateActivity"
            android:screenOrientation="landscape">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".activity.HistoryDataActivity"
            android:screenOrientation="landscape" />
        <activity
            android:name=".activity.OrderActivity"
            android:screenOrientation="landscape" />
        <activity
            android:name=".activity.FinishActivity"
            android:screenOrientation="landscape" />
        <activity android:name=".activity.OrderDetailActivity"></activity>
    </application>

</manifest>
